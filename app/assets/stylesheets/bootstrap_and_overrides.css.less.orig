/* Responsive
-------------------------------------------------- */

/* Desktop large
------------------------- */
@media (min-width: 1200px) {
  body {
  }
  
  .span9 li {
    line-height: 30px;
    margin-bottom: 10px;
  }
  
  h1 {
    color: #122549;
    font-size: 40px;
  }
  
  h2 {
    color: #A31B20;
    font-size: 30px;
  }
  
  .hero-unit {
    overflow: auto;
    padding: 40px 60px;
    
    h2 {
      margin-bottom: 40px;
    }

  }
  
  .hero-reg {
    float:left;
    margin-right:110px;
  }
  
  .hero-logo {
    float:left;
  }
  
  .convo {
    width:150px;
  }
  
  .front {
    font-size: 22px;
  }
  
  .sidenav {
    width: 258px;
  }
  
  .works li {
    line-height: 30px;
    font-size:23px;
  }
  
  .rcolumn li {
    line-height: 40px;
    font-size: 26px;
  }  
  
  label, input, button, select, textarea {
    font-size: 22px;
  }
  
  select, textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
    font-size:20px;
    color: #666;
  }
  
  .leila {
    width:550px;
    height:309px; 
  }
}

@media (max-width: 1200px) {
  .hero-logo {display:none;}
}


/* Desktop
------------------------- */
@media (max-width: 980px) {
  /* When affixed, space properly */
  .sidenav {
    top: 0;
    margin-top: 30px;
    margin-right: 0;
  }
  .hero-logo {
    display:none;
  }
}

/* Tablet to desktop
------------------------- */
@media (min-width: 768px) and (max-width: 980px) {
  /* Adjust sidenav width */
  .sidenav {
    width: 166px;
    margin-top: 20px;
  }
  .sidenav.affix {
    top: 0;
  }
  .hero-logo {
    display:none;
  }
  .foot {float:left;}
  .rachel {
    width:650px;
    height:366px; 
  }
  .leila {
    width:350px;
    height:197px; 
  }
}

/* Tablet
------------------------- */
@media (max-width: 768px) {
  /* Sidenav */
  .sidenav {
    width: auto;
    margin-bottom: 20px;
  }
  .sidenav.affix {
    position: static;
    width: auto;
    top: 0;
  }
  
  .hero-unit {
    h1 {
      font-size: 46px;
    }
    h2 {
      font-size: 26px;
    }
    padding:20px;
  }
  .hero-logo {
    display:none;
  }
  .terms {
    width: auto;
  }
  .foot {float:left;}
  .navbar-inner {
    padding-right:0px;
    padding-left:0px;
  }
  #logo {
    img {width:250px;}
  }
  
  .rachel {
    width:550px;
    height:309px; 
  }
  .leila {
    width:550px;
    height:309px; 
